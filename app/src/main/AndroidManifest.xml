<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ddwu.com.mobile.anylearn"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />


    <queries>
        <intent>
            <action android:name="android.speech.RecognitionService" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:enableOnBackInvokedCallback="true"
        android:name=".KakaoApplication"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Anylearn2"
        android:networkSecurityConfig="@xml/network_security_config"
        tools:targetApi="31">
        <receiver android:name=".AlarmReceiver" android:exported="true">
            <intent-filter>
                <action android:name="ddwu.com.mobile.anylearn.ANYLEARN_ALARM" />
            </intent-filter>
        </receiver>
        <activity
            android:name=".WithaiSelect"
            android:exported="false" />
        <activity
            android:name=".WithaiLevel"
            android:exported="false" />
        <activity
            android:name=".SignUp2"
            android:exported="false" />
        <activity
            android:name=".SignUp1"
            android:exported="false" />
        <activity
            android:name=".ScriptSaveChoice"
            android:exported="false" />
        <activity
            android:name=".ScriptNameInput"
            android:exported="false" />
        <activity
            android:name=".MyScriptList"
            android:exported="false" />
        <activity
            android:name=".MyScript"
            android:exported="false" />
        <activity
            android:name=".MyDiaryScript"
            android:exported="false" />
        <activity
            android:name=".MyDiaryMain"
            android:exported="false" />
        <activity
            android:name=".FirstSetting"
            android:exported="false" />
        <activity
            android:name=".Anylearn"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplaySettingPage"
            android:exported="false" />
        <activity
            android:name=".ModeSettingPage"
            android:exported="false" />
          <activity
            android:name=".NotificationSettingPage"
            android:exported="false" />
        <activity
            android:name=".AccountSettingPage"
            android:exported="false" />
        <activity
            android:name=".SettingPage"
            android:exported="false" />
        <activity
            android:name=".MainPage"
            android:exported="false">
        </activity>
        <activity
            android:name=".SignIn"
            android:exported="false" />
        <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Redirect URI: "kakao${NATIVE_APP_KEY}://oauth" -->
                <data android:host="oauth"
                    android:scheme="@string/kakao_oauth_host" />
            </intent-filter>
        </activity>
    </application>

</manifest>